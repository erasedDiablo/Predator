-- Roblox Fake Executor with Cmd GUI Animation + Predator Identify Screen
-- Put this LocalScript into StarterGui

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer

-- === IDENTIFY SCREEN ===
local identifyGui = Instance.new("ScreenGui", player.PlayerGui)
identifyGui.IgnoreGuiInset = true
identifyGui.ResetOnSpawn = false

local bg = Instance.new("Frame", identifyGui)
bg.Size = UDim2.new(1, 0, 1, 0)
bg.BackgroundColor3 = Color3.fromRGB(0, 0, 0)

-- Title
local titleText = Instance.new("TextLabel", bg)
titleText.AnchorPoint = Vector2.new(0.5, 0)
titleText.Position = UDim2.new(0.5, 0, 0.35, 0)
titleText.Size = UDim2.new(0.8, 0, 0, 50)
titleText.BackgroundTransparency = 1
titleText.Text = "PLEASE IDENTIFY YOURSELF"
titleText.Font = Enum.Font.Code
titleText.TextSize = 32
titleText.TextTransparency = 1
titleText.TextColor3 = Color3.fromRGB(0, 255, 255)

-- Outer glow
local inputOuter = Instance.new("Frame", bg)
inputOuter.AnchorPoint = Vector2.new(0.5, 0)
inputOuter.Position = UDim2.new(0.5, 0, 0.45, 0)
inputOuter.Size = UDim2.new(0.5, 0, 0, 40)
inputOuter.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
inputOuter.BorderSizePixel = 2
inputOuter.BorderColor3 = Color3.fromRGB(255, 100, 0)
inputOuter.BackgroundTransparency = 0.2

-- Inner cyan border
local inputInner = Instance.new("Frame", inputOuter)
inputInner.Size = UDim2.new(1, -4, 1, -4)
inputInner.Position = UDim2.new(0, 2, 0, 2)
inputInner.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
inputInner.BorderSizePixel = 2
inputInner.BorderColor3 = Color3.fromRGB(0, 255, 255)

-- Text inside
local inputText = Instance.new("TextLabel", inputInner)
inputText.Size = UDim2.new(1, -8, 1, 0)
inputText.Position = UDim2.new(0, 4, 0, 0)
inputText.BackgroundTransparency = 1
inputText.Text = ""
inputText.Font = Enum.Font.Code
inputText.TextSize = 28
inputText.TextXAlignment = Enum.TextXAlignment.Left
inputText.TextColor3 = Color3.fromRGB(0, 255, 255)

-- Fade in title
TweenService:Create(titleText, TweenInfo.new(1), {TextTransparency = 0}):Play()
task.wait(1)

-- Cursor blink
local cursorVisible = true
spawn(function()
	while identifyGui.Parent do
		if cursorVisible then
			inputText.Text = inputText.Text .. "|"
		else
			inputText.Text = string.gsub(inputText.Text, "|", "")
		end
		cursorVisible = not cursorVisible
		task.wait(0.5)
	end
end)

-- Typing Predatorrrrrrrrrrr
local fullName = "Predatorrrrrrrrrrr"
for i = 1, #fullName do
	inputText.Text = string.sub(fullName, 1, i)
	if cursorVisible then inputText.Text = inputText.Text .. "|" end
	task.wait(0.12)
end

task.wait(0.8)

-- Dramatic backspace to "Predator"
local targetName = "Predator"
while #inputText.Text:gsub("|", "") > #targetName do
	inputText.Text = string.sub(inputText.Text:gsub("|", ""), 1, -2)
	if cursorVisible then inputText.Text = inputText.Text .. "|" end
	task.wait(0.08)
end

task.wait(0.5)

-- Fade out identify screen
TweenService:Create(bg, TweenInfo.new(1), {BackgroundTransparency = 1}):Play()
TweenService:Create(titleText, TweenInfo.new(1), {TextTransparency = 1}):Play()
TweenService:Create(inputOuter, TweenInfo.new(1), {BackgroundTransparency = 1}):Play()
TweenService:Create(inputInner, TweenInfo.new(1), {BackgroundTransparency = 1}):Play()
TweenService:Create(inputText, TweenInfo.new(1), {TextTransparency = 1}):Play()
task.wait(1)
identifyGui:Destroy()

-- Roblox Fake Executor with Cmd GUI Animation (Enhanced)
-- Paste into StarterGui -> LocalScript
wait (5)

local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer

local injected = false

-- === Main GUI Frame ===
local gui = Instance.new("ScreenGui", player.PlayerGui)
gui.ResetOnSpawn = false

local mainFrame = Instance.new("Frame", gui)
mainFrame.Size = UDim2.new(0, 300, 0, 200)
mainFrame.Position = UDim2.new(0.3, 0, 0.3, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
mainFrame.BackgroundTransparency = 0.3
mainFrame.BorderSizePixel = 0
mainFrame.Name = "ExecutorFrame"
mainFrame.Active = true
mainFrame.Draggable = true
Instance.new("UICorner", mainFrame).CornerRadius = UDim.new(0, 12)

local title = Instance.new("TextLabel", mainFrame)
title.Size = UDim2.new(1, 0, 0, 40)
title.BackgroundTransparency = 1
title.Text = "Exetreme Injectorüíâ"
title.Font = Enum.Font.GothamBold
title.TextSize = 18
title.TextColor3 = Color3.fromRGB(255, 255, 255)

local scroll = Instance.new("ScrollingFrame", mainFrame)
scroll.Size = UDim2.new(1, -20, 1, -90)
scroll.Position = UDim2.new(0, 10, 0, 45)
scroll.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
scroll.BackgroundTransparency = 0.2
scroll.BorderSizePixel = 0
scroll.ScrollBarThickness = 8
scroll.AutomaticCanvasSize = Enum.AutomaticSize.Y
scroll.CanvasSize = UDim2.new(0, 0, 0, 0)
Instance.new("UICorner", scroll).CornerRadius = UDim.new(0, 8)

local codeBox = Instance.new("TextBox", scroll)
codeBox.Size = UDim2.new(1, -10, 1, -10)
codeBox.Position = UDim2.new(0, 5, 0, 5)
codeBox.BackgroundTransparency = 1
codeBox.Text = "Main,py..."
codeBox.TextXAlignment = Enum.TextXAlignment.Left
codeBox.TextYAlignment = Enum.TextYAlignment.Top
codeBox.ClearTextOnFocus = false
codeBox.MultiLine = true
codeBox.Font = Enum.Font.Code
codeBox.TextSize = 14
codeBox.TextWrapped = false
codeBox.TextColor3 = Color3.fromRGB(255, 255, 255)

-- === Buttons ===
local injectBtn = Instance.new("TextButton", mainFrame)
injectBtn.Size = UDim2.new(0.45, 0, 0, 30)
injectBtn.Position = UDim2.new(0.05, 0, 1, -35)
injectBtn.BackgroundColor3 = Color3.fromRGB(0, 102, 255)
injectBtn.Text = "Inject"
injectBtn.Font = Enum.Font.GothamBold
injectBtn.TextSize = 16
injectBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
Instance.new("UICorner", injectBtn).CornerRadius = UDim.new(0, 8)

local execBtn = Instance.new("TextButton", mainFrame)
execBtn.Size = UDim2.new(0.45, 0, 0, 30)
execBtn.Position = UDim2.new(0.5, 0, 1, -35)
execBtn.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
execBtn.Text = "Execute"
execBtn.Font = Enum.Font.GothamBold
execBtn.TextSize = 16
execBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
Instance.new("UICorner", execBtn).CornerRadius = UDim.new(0, 8)

-- === Fake Cmd GUI ===
local cmdGui = Instance.new("Frame", gui)
cmdGui.Size = UDim2.new(0, 400, 0, 300)
cmdGui.Position = UDim2.new(0.35, 0, 0.35, 0)
cmdGui.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
cmdGui.BackgroundTransparency = 0.2
cmdGui.Visible = false
cmdGui.Active = true
cmdGui.Draggable = true
Instance.new("UICorner", cmdGui).CornerRadius = UDim.new(0, 12)

local cmdTitle = Instance.new("TextLabel", cmdGui)
cmdTitle.Size = UDim2.new(1, 0, 0, 30)
cmdTitle.BackgroundTransparency = 1
cmdTitle.Text = "ìÜ©‚ò†Ô∏éÔ∏éìÜ™Predatorüêç"
cmdTitle.Font = Enum.Font.Code
cmdTitle.TextSize = 16
cmdTitle.TextColor3 = Color3.fromRGB(0, 255, 0)

local cmdScroll = Instance.new("ScrollingFrame", cmdGui)
cmdScroll.Size = UDim2.new(1, -10, 1, -40)
cmdScroll.Position = UDim2.new(0, 5, 0, 35)
cmdScroll.BackgroundTransparency = 1
cmdScroll.CanvasSize = UDim2.new(0, 0, 0, 0)
cmdScroll.ScrollBarThickness = 6
cmdScroll.AutomaticCanvasSize = Enum.AutomaticSize.Y

local cmdList = Instance.new("UIListLayout", cmdScroll)
cmdList.SortOrder = Enum.SortOrder.LayoutOrder

local function clearCmd()
	for _, child in ipairs(cmdScroll:GetChildren()) do
		if child:IsA("TextLabel") then child:Destroy() end
	end
end

local function addCmdLine(text)
	local label = Instance.new("TextLabel", cmdScroll)
	label.Size = UDim2.new(1, 0, 0, 20)
	label.BackgroundTransparency = 1
	label.Text = text
	label.Font = Enum.Font.Code
	label.TextSize = 14
	label.TextXAlignment = Enum.TextXAlignment.Left
	label.TextColor3 = Color3.fromRGB(0, 255, 0)
end

local hackingLines = {
	"[INFO] Connecting to Brain Server...",
	"[INFO] Establishing secure connection...",
	"[OK] Connection established.",
	"[INFO] Bypassing Roblox security protocols...",
	"[OK] Firewall bypassed successfully.",
	"[INFO] Injecting Lua bytecode into client...",
	"[OK] Injection complete.",
	"[INFO] Accessing encrypted player data...",
	"[OK] Data decryption success.",
	"[INFO] Breaking through security system...",
	"[OK] Admin access granted.",
	"[INFO] Installing backdoor...",
	"[OK] Backdoor active.",
	"[INFO] Running memory scan...",
	"[OK] Memory clean.",
	"[INFO] Scanning network...",
	"[OK] No threats detected.",
	"[INFO] Loading advanced scripts...",
	"[OK] Scripts loaded.",
	"[INFO] Finalizing process...",
    "[OK] Extracting BiteForce...",
	"[INFO] Bypassing Game Security...",
    "[OK] Bypass Successfully...",
    "[INFO] Breaking BrainServer...",
    "[ERROR] While Putting Backdoor...",
    "[OK] Done.",
}

local function runConsole(lines, callback)
	cmdGui.Visible = true
	clearCmd()
	task.spawn(function()
		for _, line in ipairs(lines) do
			addCmdLine(line)
			task.wait(0.25)
		end
		if callback then callback() end
	end)
end

-- Close console on Enter key
UserInputService.InputBegan:Connect(function(input, gp)
	if gp then return end
	if cmdGui.Visible and input.KeyCode == Enum.KeyCode.Return then
		cmdGui.Visible = false
	end
end)

-- Inject button
injectBtn.MouseButton1Click:Connect(function()
	if injected then
		runConsole({"[WARN] Already Injected ‚Äî Press Enter to Close"})
		return
	end
	injected = true
	injectBtn.Text = "Injecting..."
	TweenService:Create(injectBtn, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(255, 165, 0)}):Play()
	task.wait(1)
	injectBtn.Text = "Injected!"
	TweenService:Create(injectBtn, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(0, 200, 0)}):Play()
	runConsole(hackingLines, function()
		addCmdLine("[SYSTEM] Injection sequence finished ‚Äî Press Enter to Close")
	end)
end)

-- Execute button
execBtn.MouseButton1Click:Connect(function()
	local scriptCode = codeBox.Text
	if not injected then
		runConsole({"[ERROR] Failed While Breaking BrainServerüß†"})
		return
	end
	runConsole(hackingLines, function()
		addCmdLine("[SUCCESS] Script Executed Successfully ‚Äî Press Enter to Close")
		task.wait(0.5)
		if scriptCode and scriptCode ~= "" then
			local func, err = loadstring(scriptCode)
			if func then func() else warn("Error: "..err) end
		end
	end)
end)

